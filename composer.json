{
    "name": "phpunit/phpunit-dom-assertions",
    "description": "DOM assertions for PHPUnit",
    "license": "MIT",
    "keywords": [
        "dom",
        "xpath",
        "css",
        "assertions",
        "phpunit",
        "tests"
    ],
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de"
        },
        {
            "name": "Jeff Welch",
            "email": "whatthejeff@gmail.com"
        },
        {
            "name": "Lars Strojny",
            "email": "lars@strojny.net"
        }
    ],
    "homepage": "https://github.com/lstrojny/phpunit-dom-assertions",
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-dom": "*",
        "phpunit/phpunit": "^10.0.16",
        "symfony/css-selector": "^6.2.7",
        "symfony/dom-crawler": "^6.2.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.15.1",
        "phpstan/phpstan": "^1.10.6",
        "phpstan/phpstan-deprecation-rules": "^1.1.2",
        "phpstan/phpstan-phpunit": "^1.3.10"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
